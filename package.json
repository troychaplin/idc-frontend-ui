{
  "name": "@troychaplin79/idc-ui-kit",
  "description": "A library template built by Vite, React, Typescript, TailwindCSS and Storybook",
  "homepage": "https://github.com/troychaplin/idc-ui-kit#readme",
  "author": "Troy Chaplin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troychaplin/idc-ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/troychaplin/idc-ui-kit/issues"
  },
  "version": "0.0.1",
  "main": "dist/idc-ui.umd.js",
  "module": "dist/idc-ui.mjs",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/idc-ui.mjs",
      "require": "./dist/idc-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build": "tsc && vite build",
    "build:storybook": "storybook build",
    "prettier": "prettier --write .",
    "clean": "rm -rf node_modules ; rm package-lock.json ; npm install",
    "test": "echo \"No test specified\" && exit 0",
    "prepare": "husky"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@storybook/addon-a11y": "^8.4.2",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-links": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.4.2",
    "@storybook/react": "^8.4.2",
    "@storybook/react-vite": "^8.4.2",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.11.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "storybook": "^8.4.2",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0"
  }
}
